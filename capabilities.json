{
  "dataRoles": [
    {
      "displayName": "Field to bin",
      "name": "category",
      "kind": "Grouping"
    },
    {
      "displayName": "Value",
      "name": "measure",
      "kind": "Measure"
    },
    {
      "displayName": "Frequency measure",
      "name": "countMeasure",
      "kind": "Measure"
    },
    {
      "displayName": "Extra Tooltip",
      "name": "tooltips",
      "kind": "Measure"
    }
  ],
  "dataViewMappings": [
    {
      "conditions": [
        {
          "category": {
            "max": 1
          },
          "measure": {
            "max": 1
          },
          "countMeasure": {
            "max": 1
          },
          "tooltips": {
            "max": 1
          }
        }
      ],
      "categorical": {
        "categories": {
          "for": {
            "in": "category"
          },
          "dataReductionAlgorithm": {
            "top": {
              "count": 30000
            }
          }
        },
        "values": {
          "select": [
            {
              "bind": {
                "to": "measure"
              }
            },
            {
              "bind": {
                "to": "countMeasure"
              }
            },
            {
              "bind": {
                "to": "tooltips"
              }
            }
          ]        
        }
      }
    }
  ],
  "objects": {
    "bins": {
      "displayName": "Bins",
      "properties": {
